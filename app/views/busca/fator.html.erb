<h1>Relatório para os fatores <p id="demo"></p></h1>
<table class="center-align">
  <thead>
    <tr>
      <th>Diagnósticos</th>
      <th colspan="1"></th>
    </tr>
  </thead>
  <tbody>
    <% @relatorios.each do |relatorio| %>
      <tr>
        <td><%= relatorio.diagnostico.nome %></td>
       <td><%= link_to 'Ver Diagnóstico', busca_relatorio_path(:id => relatorio.diagnostico) %></td>
       <td  class="nome"><%= relatorio.fator.nome %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<script>
    window.onload((adicionarH3()));
    function adicionarH3(){
      var elements = document.getElementsByClassName('nome');
      var segura = document.getElementById("demo");
      var requiredElement = elements[0];
      var txt = requiredElement.innerHTML;
      segura.innerHTML = txt;
      }
  
</script>